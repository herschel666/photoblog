{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {},
    "rules": {
        "comma-dangle": 2,
        "indent": [2, 4, {"SwitchCase": 0}],
        "react/jsx-indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "block-scoped-var": 0,
        "padded-blocks": 0,
        "no-mixed-operators": 0
    },
    "parserOptions": {},
    "plugins": ["react"]
}
