{
  "name": "photoblog",
  "version": "0.1.0",
  "description": "Phox-powered photoblog",
  "main": "server.js",
  "scripts": {
    "clean": "./script/clean",
    "tsc": "tsc",
    "phox": "phox",
    "lint": "tslint --type-check --format stylish --project tsconfig.json",
    "test": "npm-run-all lint build",
    "watch": "concurrently \"tsc --watch\" \"node server.js\"",
    "build": "npm-run-all clean tsc phox",
    "postinstall":
      "rm -f node_modules/@types/autoprefixer/node_modules/postcss/lib/postcss.d.ts"
  },
  "keywords": ["photography"],
  "author": "Emanuel Kluge (https://www.emanuel-kluge.de/)",
  "license": "MIT",
  "dependencies": {
    "@types/aphrodite": "^0.5.6",
    "@types/autoprefixer": "^6.7.3",
    "@types/classnames": "^2.2.3",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/next": "^2.4.2",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/webpack": "^3.0.11",
    "aphrodite": "^1.2.4",
    "autoprefixer": "^7.1.4",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "classnames": "^2.2.5",
    "concurrently": "^3.5.0",
    "isomorphic-fetch": "^2.2.1",
    "next": "^3.2.2",
    "npm-run-all": "^4.1.1",
    "phox": "0.1.0-alpha.25",
    "postcss": "^6.0.11",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.2"
  }
}
