{
  "name": "photoblog",
  "version": "0.1.0",
  "description": "Phox-powered photoblog",
  "main": "server.js",
  "scripts": {
    "clean": "./script/clean",
    "tsc": "tsc",
    "phox": "phox",
    "lint": "tslint --type-check --format stylish --project tsconfig.json",
    "test": "npm run lint && npm run build",
    "watch": "concurrently \"tsc --watch\" \"node server.js\"",
    "build": "npm-run-all clean tsc phox"
  },
  "keywords": [
    "photography"
  ],
  "author": "Emanuel Kluge (https://www.emanuel-kluge.de/)",
  "license": "MIT",
  "dependencies": {
    "@types/aphrodite": "^0.5.6",
    "@types/classnames": "^2.2.3",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/next": "^2.4.2",
    "@types/ramda": "^0.24.12",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "aphrodite": "^1.2.4",
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.1",
    "next": "^3.2.2",
    "phox": "0.1.0-alpha.25",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "npm-run-all": "^4.1.1",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.5.2"
  }
}
