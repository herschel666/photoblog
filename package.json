{
  "name": "photoblog",
  "version": "0.1.0",
  "description": "Gatsby-powered photoblog",
  "private": true,
  "author": "Emanuel Kluge (https://www.emanuel-kluge.de/)",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.12",
    "classnames": "2.2.6",
    "emoji-regex": "9.2.0",
    "fast-exif": "1.0.1",
    "fast-glob": "3.2.4",
    "gatsby": "2.24.92",
    "gatsby-image": "2.4.21",
    "gatsby-plugin-feed": "2.6.0",
    "gatsby-plugin-layout": "1.3.13",
    "gatsby-plugin-netlify": "2.4.0",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-sharp": "2.7.0",
    "gatsby-plugin-typescript": "2.5.0",
    "gatsby-plugin-webpack-size": "1.0.0",
    "gatsby-remark-images": "3.4.0",
    "gatsby-source-contentful": "3.1.0",
    "gatsby-source-filesystem": "2.4.0",
    "gatsby-transformer-remark": "2.9.0",
    "gatsby-transformer-sharp": "2.5.21",
    "md5-file": "5.0.0",
    "mime": "2.4.6",
    "pretty-bytes": "5.4.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "slash": "3.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "11.1.0",
    "@types/classnames": "2.2.11",
    "@types/jest": "26.0.15",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "@wdio/cli": "6.7.2",
    "@wdio/jasmine-framework": "6.7.2",
    "@wdio/local-runner": "6.7.2",
    "@wdio/spec-reporter": "6.7.0",
    "archiver": "5.0.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-preset-gatsby": "0.5.15",
    "chromedriver": "86.0.0",
    "dotenv-load": "2.0.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "node-fetch": "2.6.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.1.2",
    "typescript": "4.0.5",
    "wdio-chromedriver-service": "6.0.4"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "predeploy": "TARGET=production npm run build -- --prefix-paths",
    "deploy": "node ./script/deploy.js",
    "type-check": "tsc",
    "lint-js": "eslint '**/*.js' --config .eslintrc.js --quiet --fix --ignore-pattern '**/*.{ts,tsx}' --ignore-path .gitignore",
    "lint-ts": "eslint 'src/**/*.{ts,tsx}' --config .eslintrc.ts.js --quiet --fix --ignore-pattern '**/*.js' --ignore-path .gitignore",
    "fmt:code": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write \"**/**.{js,jsx,ts,tsx,css}\"",
    "fmt:content": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write \"_*/**/**.md\"",
    "fmt": "npm-run-all fmt:code fmt:content",
    "fmt:list": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --list-different \"**/**.{js,jsx,ts,tsx}\"",
    "lint": "npm-run-all type-check lint-ts lint-js",
    "jest": "jest",
    "test": "npm-run-all fmt:list lint jest build e2e",
    "start": "gatsby develop",
    "serve": "gatsby serve -p 8000",
    "e2e": "./script/wdio"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herschel666/photoblog"
  },
  "bugs": {
    "url": "https://github.com/herschel666/photoblog/issues"
  }
}
