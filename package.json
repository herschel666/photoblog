{
  "name": "photoblog",
  "version": "0.1.0",
  "description": "Gatsby-powered photoblog",
  "private": true,
  "author": "Emanuel Kluge (https://www.emanuel-kluge.de/)",
  "dependencies": {
    "@types/classnames": "2.2.9",
    "@types/react-helmet": "5.0.15",
    "archiver": "3.1.1",
    "classnames": "2.2.6",
    "dotenv-load": "2.0.0",
    "emoji-regex": "8.0.0",
    "fast-exif": "1.0.1",
    "fast-glob": "3.1.1",
    "gatsby": "2.18.23",
    "gatsby-image": "2.2.38",
    "gatsby-plugin-feed": "2.3.26",
    "gatsby-plugin-layout": "1.1.21",
    "gatsby-plugin-netlify": "2.1.31",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-sharp": "2.3.13",
    "gatsby-plugin-typescript": "2.1.26",
    "gatsby-remark-images": "3.1.42",
    "gatsby-source-contentful": "2.1.77",
    "gatsby-source-filesystem": "2.1.46",
    "gatsby-transformer-remark": "2.6.48",
    "gatsby-transformer-sharp": "2.3.12",
    "md5-file": "4.0.0",
    "mime": "2.4.4",
    "node-fetch": "2.6.0",
    "pretty-bytes": "5.3.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "slash": "3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.16.0",
    "@typescript-eslint/parser": "2.16.0",
    "@wdio/cli": "5.18.5",
    "@wdio/jasmine-framework": "5.16.15",
    "@wdio/local-runner": "5.18.5",
    "@wdio/spec-reporter": "5.16.11",
    "babel-eslint": "10.0.3",
    "chromedriver": "78.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "typescript": "3.7.4",
    "wdio-chromedriver-service": "5.0.2"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "predeploy": "NODE_ENV=production npm run build -- --prefix-paths",
    "deploy": "node ./script/deploy.js",
    "type-check": "tsc",
    "lint-js": "eslint '**/*.js' --config .eslintrc.js --quiet --fix --ignore-pattern '**/*.{ts,tsx}' --ignore-path .gitignore",
    "lint-ts": "eslint 'src/**/*.{ts,tsx}' --config .eslintrc.ts.js --quiet --fix --ignore-pattern '**/*.js' --ignore-path .gitignore",
    "fmt": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write \"**/**.{js,jsx,ts,tsx}\"",
    "lint": "npm-run-all type-check lint-ts lint-js",
    "test": "npm-run-all lint build e2e",
    "start": "gatsby develop",
    "serve": "gatsby serve -p 8000",
    "e2e": "./script/wdio"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herschel666/photoblog"
  },
  "bugs": {
    "url": "https://github.com/herschel666/photoblog/issues"
  }
}
