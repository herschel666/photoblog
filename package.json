{
  "name": "photoblog",
  "version": "0.1.0",
  "description": "Phox-powered photoblog",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rfv ./{dist,.next}/",
    "phox": "phox",
    "lint": "tsc --noEmit && tslint --format stylish --project tsconfig.json",
    "fmt": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write \"**/**.{js,jsx,ts,tsx}\"",
    "test": "npm-run-all lint build",
    "start": "node server.js",
    "build": "npm-run-all clean phox"
  },
  "keywords": [
    "photography",
    "react",
    "next.js"
  ],
  "author": "Emanuel Kluge (https://www.emanuel-kluge.de/)",
  "license": "MIT",
  "dependencies": {
    "@types/aphrodite": "^0.5.13",
    "@types/autoprefixer": "^6.7.3",
    "@types/classnames": "^2.2.3",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/next": "^8.0.1",
    "@types/qs": "^6.5.0",
    "@types/react": "16.1.0",
    "@types/react-dom": "16.0.4",
    "@zeit/next-typescript": "^1.1.1",
    "aphrodite": "^2.3.1",
    "autoprefixer": "^7.1.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "classnames": "^2.2.5",
    "cssnano": "3.10.0",
    "date-fns": "1.30.1",
    "isomorphic-fetch": "^2.2.1",
    "next": "^8.0.3",
    "npm-run-all": "^4.1.1",
    "phox": "0.6.0-rc.8",
    "postcss": "^6.0.11",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.6",
    "qs": "^6.5.1",
    "raw-loader": "^0.5.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.1.0",
    "typescript": "^3.3.3",
    "webpack": "^4.29.0"
  },
  "devDependencies": {
    "prettier": "^1.16.4",
    "tslint-react": "^3.6.0"
  }
