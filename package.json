{
  "name": "photoblog",
  "version": "0.1.0",
  "description": "Gatsby-powered photoblog",
  "private": true,
  "author": "Emanuel Kluge (https://www.emanuel-kluge.de/)",
  "dependencies": {
    "@types/classnames": "2.2.10",
    "@types/react-helmet": "6.0.0",
    "archiver": "4.0.1",
    "classnames": "2.2.6",
    "dotenv-load": "2.0.0",
    "emoji-regex": "9.0.0",
    "fast-exif": "1.0.1",
    "fast-glob": "3.2.4",
    "gatsby": "2.23.7",
    "gatsby-image": "2.4.7",
    "gatsby-plugin-feed": "2.5.5",
    "gatsby-plugin-layout": "1.3.4",
    "gatsby-plugin-netlify": "2.3.5",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-plugin-sharp": "2.6.12",
    "gatsby-plugin-typescript": "2.4.6",
    "gatsby-remark-images": "3.3.11",
    "gatsby-source-contentful": "2.3.16",
    "gatsby-source-filesystem": "2.3.12",
    "gatsby-transformer-remark": "2.8.17",
    "gatsby-transformer-sharp": "2.5.5",
    "md5-file": "5.0.0",
    "mime": "2.4.6",
    "node-fetch": "2.6.0",
    "pretty-bytes": "5.3.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "slash": "3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "@wdio/cli": "6.2.0",
    "@wdio/jasmine-framework": "6.2.0",
    "@wdio/local-runner": "6.2.0",
    "@wdio/spec-reporter": "6.1.23",
    "babel-eslint": "10.1.0",
    "chromedriver": "83.0.1",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "typescript": "3.9.5",
    "wdio-chromedriver-service": "6.0.3"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "predeploy": "NODE_ENV=production npm run build -- --prefix-paths",
    "deploy": "node ./script/deploy.js",
    "type-check": "tsc",
    "lint-js": "eslint '**/*.js' --config .eslintrc.js --quiet --fix --ignore-pattern '**/*.{ts,tsx}' --ignore-path .gitignore",
    "lint-ts": "eslint 'src/**/*.{ts,tsx}' --config .eslintrc.ts.js --quiet --fix --ignore-pattern '**/*.js' --ignore-path .gitignore",
    "fmt:code": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write \"**/**.{js,jsx,ts,tsx,css}\"",
    "fmt:content": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --write \"_*/**/**.md\"",
    "fmt": "npm-run-all fmt:code fmt:content",
    "fmt:list": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --list-different \"**/**.{js,jsx,ts,tsx}\"",
    "lint": "npm-run-all type-check lint-ts lint-js",
    "test": "npm-run-all fmt:list lint build e2e",
    "start": "gatsby develop",
    "serve": "gatsby serve -p 8000",
    "e2e": "./script/wdio"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/herschel666/photoblog"
  },
  "bugs": {
    "url": "https://github.com/herschel666/photoblog/issues"
  }
}
